<Window x:Class="PropertyMgmt.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:ignore="http://www.ignore.com"
        mc:Ignorable="d ignore"
        Height="300"
        Width="400"
        Title="Zebra Software Solutions Property Manager v0.1"
        DataContext="{Binding Main, Source={StaticResource Locator}}">
    
    <Window.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Skins/MainSkin.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Window.Resources>

    <Grid x:Name="LayoutRoot">
        <Button Content="Properties" HorizontalAlignment="Left" Margin="101,72,0,0" VerticalAlignment="Top" Width="85" Click="Button_Click"/>
        <Button Name="btnAdministration" Content="Administration" HorizontalAlignment="Left" Margin="101,27,0,0" VerticalAlignment="Top" Width="85" Click="btnAdministration_Click"/>
    </Grid>
</Window>
